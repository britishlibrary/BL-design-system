{
  "name": "@britishlibrary/icon-library",
  "version": "1.0.2",
  "description": "A library of icons for the British Library",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "author": "The British Library",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/britishlibrary/BL-design-system",
    "directory": "packages/icon-library"
  },
  "scripts": {
    "build": "run-s build-icons exports build:es build:cjs types",
    "build:es": "BABEL_ENV=es babel src --out-dir \"dist/es\" --extensions \".ts,.tsx\"",
    "build:cjs": "BABEL_ENV=cjs webpack --config=webpack/production.js",
    "build-icons": "rm -rf src/icons; svgr -d src/icons -- src/assets/icons/**/; rm src/icons/index.ts",
    "exports": "./generate-exports.sh",
    "types": "tsc --emitDeclarationOnly --declarationMap --declaration --noEmit false --allowJs false --outDir dist/types",
    "prepare": "yarn build"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "prettier --write"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@inline-svg-unique-id/react": "^1.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@britishlibrary/eslint-config": "^1.0.2",
    "@svgr/cli": "^6.1.2",
    "@types/react": "^17.0.38",
    "babel-loader": "^9.0.1",
    "babel-plugin-react-inline-svg-unique-id": "^1.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "source-map-loader": "^4.0.1",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.1.2",
    "webpack": "^5.52.1",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.2.0"
  },
  "gitHead": "ba411e847be4d30dd60c2a5bd8394243ebefde8d"
}
